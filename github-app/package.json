{
  "name": "github-app",
  "version": "1.0.0",
  "private": true,
  "description": "rescribe github integration",
  "author": "reScribe <maintainer@rescribe.com>",
  "license": "CC-BY-NC-SA-4.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rescribe-dev/rescribe.git"
  },
  "homepage": "https://docs.rescribe.dev",
  "bugs": "https://github.com/rescribe/rescribe/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "start": "probot run ./lib/index",
    "dev": "tsc-watch --onSuccess \"npm run postbuildWatch\"",
    "clean": "rimraf lib dist.zip && rimraf coverage",
    "format": "prettier --config prettier.config.js --ignore-path .gitignore --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "precommit": "npm run lint && npm run format",
    "prepublishOnly": "npm run codegen && npm run build",
    "printTypescriptVersion": "echo Using TypeScript && tsc --version",
    "prebuild": "npm run format && npm run lint && npm run printTypescriptVersion",
    "build": "tsc",
    "build:package": "./package.sh",
    "test": "jest",
    "coverage": "jest --coverage",
    "postbuildWatch": "npm run lint && npm run format && npm run printTypescriptVersion && npm start",
    "watch:test": "jest --watch",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@probot/serverless-lambda": "^0.5.3",
    "apollo-boost": "^0.4.9",
    "dotenv": "^8.2.0",
    "graphql": "^15.4.0",
    "http-status-codes": "^2.1.4",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.2",
    "probot": "^10.9.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.19.0",
    "@graphql-codegen/fragment-matcher": "^2.0.0",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-document-nodes": "^1.17.8",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.18.0",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-type-graphql": "^1.18.1",
    "@types/aws-lambda": "^8.10.64",
    "@types/bunyan": "^1.8.6",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.31",
    "@types/node": "^14.14.5",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.3",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "smee-client": "^1.2.2",
    "standard": "^15.0.0",
    "ts-jest": "^26.4.3",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.5"
  },
  "resolutions": {
    "graphql-tag": "2.11.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
