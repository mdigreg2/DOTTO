  "scripts": {
    "build:gatsby": "cd gatsby && tsc",
    "prebuild": "yarn precommit && yarn build:gatsby",
    "gatsby:build": "gatsby build",
    "build": "yarn remove:dist && yarn gatsby:build && yarn move:folder && yarn copy:headers",
    "predeploy": "yarn build",
    "subdomain:ignore": "mv -f dist/robots_subdomain.txt dist/robots.txt",
    "move:folder": "mv public dist",
    "copy:headers": "cp dist/_headers ../aws/cloudfront/frontend",
    "dev": "yarn prebuild && gatsby develop --host=0.0.0.0 --port=8000",
    "serve": "node static-server.js",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "remove:dist": "rimraf dist",
    "clean": "gatsby clean && yarn remove:dist && rimraf gatsby/.gatsby && rimraf dist-storybook",
    "precommit": "yarn lint && yarn codegen && yarn codegenREST",
    "storybook": "start-storybook -p 9000 -c .storybook",
    "build:storybook": "build-storybook -c .storybook -o dist-storybook",
    "serve:storybook": "node static-server.js dist-storybook 9001",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegenREST": "swagger-to-ts ../api/swagger.yml --output src/lib/generated/apiREST.ts"
  },
