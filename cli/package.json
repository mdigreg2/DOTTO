{
  "name": "@rescribe/cli",
  "version": "0.0.12",
  "description": "reScribe cli",
  "license": "CC-BY-NC-SA-4.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rescribe-dev/rescribe.git"
  },
  "author": {
    "name": "rescribe"
  },
  "keywords": [
    "cli"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "node lib/index -h",
    "dev": "tsc-watch --onSuccess \"npm run postbuildWatch\"",
    "clean": "rimraf lib && rimraf coverage && rimraf *.logs*",
    "format": "prettier --config prettier.config.js --ignore-path .gitignore --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "precommit": "npm run lint && npm run format",
    "prepublishOnly": "npm run build && cp -R lib dist",
    "printTypescriptVersion": "echo Using TypeScript && tsc --version",
    "prebuild": "npm run format && npm run lint && npm run printTypescriptVersion",
    "build": "npm run codegen && tsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "postbuildWatch": "npm run lint && npm run format && npm run printTypescriptVersion && npm start",
    "watch:test": "jest --watch",
    "package": "pkg . --out-path=dist-bin",
    "postinstall": "./postinstall.sh",
    "build:package": "npm run build && ./package.sh",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "axios": "^0.19.2",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "cosmiconfig": "^6.0.0",
    "cross-fetch": "^3.0.5",
    "dotenv": "^8.2.0",
    "exit-hook": "^2.2.0",
    "form-data": "^3.0.0",
    "glob": "^7.1.6",
    "graphql": "15.3.0",
    "graphql-tag": "^2.11.0",
    "isbinaryfile": "^4.0.6",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "krb5": "^0.5.4",
    "log4js": "^6.3.0",
    "mime-types": "^2.1.27",
    "mongodb": "^3.5.9",
    "nodegit": "^0.27.0",
    "progress": "^2.0.3",
    "prompts": "^2.3.2",
    "subscriptions-transport-ws": "^0.9.17",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.6",
    "@graphql-codegen/fragment-matcher": "^1.17.6",
    "@graphql-codegen/introspection": "1.17.6",
    "@graphql-codegen/typescript": "1.17.6",
    "@graphql-codegen/typescript-document-nodes": "^1.17.6",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.17.6",
    "@graphql-codegen/typescript-operations": "^1.17.6",
    "@graphql-codegen/typescript-type-graphql": "^1.17.6",
    "@types/glob": "^7.1.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^26.0.7",
    "@types/js-yaml": "^3.12.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mime-types": "^2.1.0",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.27",
    "@types/nodegit": "^0.26.7",
    "@types/ws": "^7.2.6",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.0.3",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.2.1",
    "pkg": "^4.4.9",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "bin": {
    "rescribe": "dist/index.js"
  },
  "pkg": {
    "assets": [
      "./node_modules/@types",
      "./node_modules/typescript/lib/*.d.ts",
      "src/**/*.ts",
      "./tsconfig.json"
    ]
  }
}
