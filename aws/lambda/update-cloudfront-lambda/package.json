{
  "name": "upload-lambda-cloudfront",
  "version": "1.0.0",
  "description": "upload lambda functions cloudfront",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "node lib",
    "clean": "rimraf lib && rimraf coverage && rimraf *.logs*",
    "format": "prettier --config prettier.config.js --ignore-path .gitignore --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "precommit": "npm run lint && npm run format",
    "printTypescriptVersion": "echo Using TypeScript && tsc --version",
    "prebuild": "npm run format && npm run lint && npm run printTypescriptVersion",
    "build": "tsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch:test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rescribe-dev/rescribe.git"
  },
  "keywords": [
    "frontend",
    "s3"
  ],
  "author": "Joshua Schmidt",
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/rescribe-dev/rescribe/issues"
  },
  "homepage": "https://github.com/rescribe-dev/rescribe#readme",
  "dependencies": {
    "aws-sdk": "^2.751.0",
    "dotenv": "^8.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "bin": "lib/index.js",
  "devDependencies": {
    "@types/mime-types": "^2.1.0",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.4.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
