{
  "name": "update-sitemap",
  "version": "0.0.1",
  "description": "update the sitmap files",
  "license": "CC-BY-NC-SA-4.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rescribe-dev/rescribe.git"
  },
  "author": {
    "name": "rescribe"
  },
  "keywords": [
    "backend",
    "edge"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "node lib/index",
    "dev": "tsc-watch --onSuccess \"npm run postbuildWatch\"",
    "clean": "rimraf lib && rimraf coverage",
    "format": "prettier --config prettier.config.js --ignore-path .gitignore --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "precommit": "npm run lint && npm run format",
    "predeploy": "npm run build && ./build_zip.sh",
    "prepublishOnly": "npm run build",
    "printTypescriptVersion": "echo Using TypeScript && tsc --version",
    "prebuild": "npm run format && npm run lint && npm run printTypescriptVersion",
    "build": "yarn clean && tsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "postbuildWatch": "npm run lint && npm run format && npm run printTypescriptVersion && npm start",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.4.1",
    "axios": "^0.20.0",
    "class-validator": "^0.12.2",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "exit-hook": "^2.2.0",
    "graphql": "^15.3.0",
    "http-status-codes": "^2.1.4",
    "log4js": "^6.3.0",
    "mongoose": "^5.10.8",
    "type-graphql": "^1.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.63",
    "@types/jest": "^26.0.14",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.7",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "aws-lambda": "^1.0.6",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.6.3",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.2",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
