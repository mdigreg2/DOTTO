{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ECAllowSpecific",
      "Effect": "Allow",
      "Action": [
        "elasticache:*"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::rescribe-repositories",
        "arn:aws:s3:::rescribe-repositories/*",
        "arn:aws:s3:::rescribe-email-templates",
        "arn:aws:s3:::rescribe-email-templates/*"
      ]
    }
  ]
}