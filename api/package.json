{
  "name": "api",
  "version": "0.0.1",
  "description": "api",
  "license": "CC-BY-NC-SA-4.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rescribe-dev/rescribe.git"
  },
  "author": {
    "name": "rescribe"
  },
  "keywords": [
    "backend",
    "api"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "node --trace-deprecation lib/index",
    "dev": "yarn prebuild && tsc-watch --onSuccess \"yarn postbuildWatch\"",
    "clean": "rimraf lib && rimraf coverage",
    "format": "prettier --config prettier.config.js --ignore-path .gitignore --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "precommit": "yarn lint && yarn format && yarn swagger && yarn codegen",
    "predeploy": "yarn build",
    "prepublishOnly": "yarn build",
    "printTypescriptVersion": "echo Using TypeScript && tsc --version",
    "prebuild": "yarn precommit && yarn printTypescriptVersion",
    "build": "tsc",
    "copy-graphql": "rm -rf lib/schema && cp -r src/schema lib/schema",
    "test": "jest",
    "coverage": "jest --coverage",
    "postbuildWatch": "yarn printTypescriptVersion && npm start",
    "watch:test": "jest --watch",
    "swagger": "swaggerGen -c ./swaggerConf.yml && cp dist_swagger/swagger.yaml swagger.yml",
    "codegenNLP": "swagger-to-ts ../nlp/deployment/swagger.yml --output src/generated/nlp.ts",
    "codegen": "yarn codegenNLP"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.10.0",
    "@googlemaps/google-maps-services-js": "^3.1.13",
    "@octokit/auth-app": "^2.10.5",
    "@octokit/auth-oauth-app": "^3.0.7",
    "@octokit/graphql": "^4.5.8",
    "@octokit/request": "^5.4.12",
    "@sendgrid/client": "^7.4.0",
    "@typegoose/typegoose": "^7.4.5",
    "apollo-server-express": "^2.19.0",
    "argon2": "^0.27.1",
    "aws-sdk": "^2.814.0",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cosmiconfig": "^7.0.0",
    "crypto-random-string": "^3.3.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "exit-hook": "^2.2.0",
    "express": "^4.17.1",
    "express-status-monitor": "^1.3.3",
    "file-type": "^16.0.1",
    "graphql": "^15.4.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-fields": "^2.0.3",
    "graphql-redis-subscriptions": "^2.3.1",
    "graphql-upload": "^11.0.0",
    "handlebars": "^4.7.6",
    "http-status-codes": "^2.1.4",
    "isbinaryfile": "^4.0.6",
    "isbot": "^3.0.20",
    "js-yaml": "^3.14.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mongoose": "^5.11.8",
    "nanoid": "^3.1.20",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.26.3",
    "sprintf-js": "^1.1.2",
    "stripe": "^8.129.0",
    "type-graphql": "^1.1.1",
    "typescript-rest": "^3.0.2",
    "typescript-rest-swagger": "^1.1.5"
  },
  "devDependencies": {
    "@manifoldco/swagger-to-ts": "^2.1.0",
    "@types/color": "^3.0.1",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.9",
    "@types/express-status-monitor": "^1.2.2",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/graphql-fields": "^1.3.3",
    "@types/ioredis": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/js-yaml": "^3.12.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.3",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "graphql": "^15.4.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
