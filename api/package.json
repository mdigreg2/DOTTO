{
  "name": "backend",
  "version": "0.0.1",
  "description": "backend",
  "license": "CC-BY-NC-SA-4.0",
  "repository": "",
  "author": {
    "name": "rescribe"
  },
  "keywords": [
    "backend",
    "api"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "node lib/index",
    "dev": "npm run precommit && npm run build && npm run start",
    "clean": "rimraf lib && rimraf coverage",
    "format": "prettier --config prettier.config.js --ignore-path .gitignore --write \"{src,__tests__}/**/*.ts\"",
    "lint": "tslint --force --format verbose \"src/**/*.ts\" --fix",
    "precommit": "npm run clean && npm run lint && npm run format",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty && npm run copy-graphql",
    "copy-graphql": "cp -r src/schemas lib/schemas",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch": "npm run build -- --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.6.1",
    "apollo-server-express": "^2.12.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-tools": "^5.0.0",
    "graphql-upload": "^10.0.0",
    "http-status-codes": "^1.4.0",
    "log4js": "^6.2.1",
    "merge-graphql-schemas": "^1.7.8",
    "mongodb": "^3.5.6",
    "nanoid": "^3.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.6",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/jest": "^23.3.3",
    "@types/mongodb": "^3.5.10",
    "@types/node": "^10.11.4",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
