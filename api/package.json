    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "exit-hook": "^2.2.0",
    "express": "^4.17.1",
    "express-status-monitor": "^1.3.3",
    "graphql-depth-limit": "^1.1.0",
    "graphql-fields": "^2.0.3",
    "graphql-redis-subscriptions": "^2.2.1",
    "graphql-tools": "^6.0.11",
    "graphql-upload": "^11.0.0",
    "handlebars": "^4.7.6",
    "http-status-codes": "^1.4.0",
