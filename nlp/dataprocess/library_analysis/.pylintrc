# pylint config

[MESSAGES CONTROL]
disable =
    too-many-instance-attributes,
    invalid-name,
    line-too-long,
    too-many-arguments,
    too-many-locals,
    broad-except,
    wrong-import-order,
    global-statement,
    unused-variable,
    redefined-outer-name,
    global-variable-not-assigned,
    import-outside-toplevel,
    import-error,
    line-too-long,
    no-else-return,
    wrong-import-position,
    duplicate-code,
    fixme
