{
	"name": "vs",
	"displayName": "rescribe",
	"description": "",
	"version": "0.0.1",
	"publisher": "reScribe",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.rescribe",
		"onCommand:extension.traverse"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.rescribe",
				"title": "reScribe",
				"when": "editorTextFocus"
				
			},
			{
				"command": "extension.addToDict",
				"title": "Add To Dict",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.traverse",
				"title": "Traverse",
				"when": "editorTextFocus"
			}
		],
		"keybindings": [
			{
				"command": "extension.rescribe",
				"key": "ctrl+r",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.addToDict",
				"key": "ctrl+e",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"python-shell": "^1.0.8"
	}
}
